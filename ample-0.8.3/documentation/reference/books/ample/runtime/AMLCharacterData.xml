						<description>
							<locale>
							</locale>
						</description>
					</argument>
					<argument name="sValue" type="String" required="true">
						<description>
							<locale>
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Replaces portion of data.
					</locale>
					<locale xml:lang="ru">
