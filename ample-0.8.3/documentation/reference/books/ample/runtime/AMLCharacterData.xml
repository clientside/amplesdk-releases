<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../../../stylesheets/page.xsl"?>
<object name="AMLCharacterData" extends="runtime:AMLNode">
	<abstract>
		<locale>
			Base object for text nodes.
		</locale>
		<locale xml:lang="ru">
			Текстовые данные.
		</locale>
	</abstract>
	<remarks />
	<examples/>
	<members>
		<methods>
			<method name="appendData" type="Void">
