						<description>
							<locale>
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Inserts portion of data at the specified position.
					</locale>
					<locale xml:lang="ru">
						Вставляет часть данных после указаной позиции.
					</locale>
				</description>
			</method>
			<method name="replaceData" type="Void">
				<arguments>
					<argument name="nOffset" type="Number" required="true">
						<description>
							<locale>
							</locale>
						</description>
					</argument>
